msgstr "Datu-multzo bat edo gehiago kentzen ditu"

#: ../tools/gda-sql.c:2398
#, c-format
msgid "%s <DATASET NAME> <DATASET NAME>"
msgstr "%s <DATU-MULTZOA_IZENA> <DATU-MULTZOA_IZENA>"

