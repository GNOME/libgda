            <doc xml:whitespace="preserve">the SQL string to parse</doc>
            <type name="utf8" c:type="gchar*"/>
          </parameter>
          <parameter name="remain"
                     direction="out"
                     caller-allocates="0"
                     transfer-ownership="full"
